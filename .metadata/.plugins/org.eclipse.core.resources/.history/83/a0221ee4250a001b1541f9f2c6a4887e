import java.util.Scanner;

public class main {

	public static void main(String[] args)  {
		
	for (int i = 0; i < 2; i++){
		System.out.println("First level"+i);
			for (int j = 0; i < 2; j++) {
				System.out.println("Second level"+j);
				for (int k = 0; i < 2; k++) {					
					System.out.println("Deepest"+k);
				}
			}
		}
}
}