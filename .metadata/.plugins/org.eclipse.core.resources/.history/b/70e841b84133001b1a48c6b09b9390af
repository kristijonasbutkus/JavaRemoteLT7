import javax.swing.Box;
import javax.swing.Icon;
import javax.swing.ImageIcon;
import javax.swing.JButton;
import javax.swing.JFrame;
public class runner {
   public static void main(String[] args) {
      JButton button1 = new JButton("One");
      Icon icon_attack_style = new ImageIcon("D:\\Java stuff\\SibirDiary\\src\\attackStyle.JPG");
      Icon icon_skills = new ImageIcon("D:\\Java stuff\\SibirDiary\\src\\skills.JPG");
      JButton attack_styles = new JButton(icon_attack_style);
      JButton skills = new JButton(icon_skills);
      Box box = Box.createHorizontalBox();

      box.add(attack_styles);
      box.add(skills);
      
      JFrame frame = new JFrame();
      frame.add(box);
      frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
      frame.setLocationByPlatform(true);
      frame.setSize(500, 300);
      frame.setVisible(true);
   }
}